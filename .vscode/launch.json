    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--batch_size",
                "256",
                "erdos_renyi_lingauss",
                "--num_variables",
                "5",
                "--num_edges",
                "5",
                "--num_samples",
                "100"
            ]
        }
    ]
}